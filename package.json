{
  "name": "react-ts-mobx-material",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@capacitor/android": "^2.4.0",
    "@capacitor/cli": "^2.4.0",
    "@capacitor/core": "^2.4.0",
    "@ionic-native/core": "^5.27.0",
    "@ionic-native/http": "^5.27.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/styles": "^4.10.0",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "axios": "^0.19.2",
    "cordova-plugin-advanced-http": "^3.0.0",
    "cordova-plugin-file": "^6.0.2",
    "mobx": "^5.15.4",
    "mobx-react": "^6.2.5",
    "mobx-react-router": "^4.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:once": "react-scripts test --watchAll=false",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:watch": "npm test -- --watch",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "npm-check": "npx npm-check -i api -i Injector -i store -i component -i lib -i App"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "src/index.tsx",
      "src/Injector.tsx",
      "src/api",
      "src/lib",
      ".stories.tsx$"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@types/jest": "^26.0.8",
    "eslint-config-react-app": "^5.2.1",
    "npm-check": "^5.9.2"
  }
}
